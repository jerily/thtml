<import name="Master" src="master.thtml" />

<Master pagetitle="My Blog">
    <div class="blog">
        <h1>My Blog</h1>
        <p>Here is my blog.</p>
        <tpl foreach="post" in="$posts">
            <div class="post">
                <h2>${post.title}</h2>
                <p>${post.content}</p>
            </div>
        </tpl>
    </div>
</Master>